../samtools-1.5/samtools index  ./aligned_GATK/Pr_1024_Plate1_B.bam
rm ./aligned_GATK/Pr_1024_Plate1_A.sam
rm ./aligned_GATK/Pr_1024_Plate1_B.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00363.11.HGCL5BBXX.6\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1026" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1026_Plate1_A.fastq.gz > ./aligned_GATK/Pr_1026_Plate1_A.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00317.73.HHJWTBBXX.4\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1026" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1026_Plate1_B.fastq.gz > ./aligned_GATK/Pr_1026_Plate1_B.sam
