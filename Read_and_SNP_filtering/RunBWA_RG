#!/bin/csh
#$ -M <email>
#$ -m abe
#$ -r y
#$ -q long
#$ -pe smp 32
#$ -N RunBWA
set path = ( $path /bwa-0.7.15 )

## index the reference genome
./bwa index ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa

##align the reads to the reference using default settings
module load bio/samtools/1.2

bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:224:H3WTYBBXX:3\tPL:illumina\tPI:450\tLB:HiSeq1\tSM:Pr_1" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq1/Pieris_HiSeq1_F_cleaned76_Pr_1.fastq.gz > ./aligned_GATK/Pr_1_HiSeq1.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1_HiSeq1.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1_HiSeq1.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1_HiSeq1.bam
rm ./aligned_GATK/Pr_1_HiSeq1.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:224:H3WTYBBXX:3\tPL:illumina\tPI:450\tLB:HiSeq1\tSM:Pr_10" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq1/Pieris_HiSeq1_F_cleaned76_Pr_10.fastq.gz > ./aligned_GATK/Pr_10_HiSeq1.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_10_HiSeq1.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_10_HiSeq1.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_10_HiSeq1.bam
rm ./aligned_GATK/Pr_10_HiSeq1.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:227:H52T5BBXX:1\tPL:illumina\tPI:450\tLB:HiSeq2\tSM:Pr_1005" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq2/Pieris_HiSeq2_F_cleaned76_Pr_1005.fastq.gz > ./aligned_GATK/Pr_1005_HiSeq2.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1005_HiSeq2.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1005_HiSeq2.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1005_HiSeq2.bam
rm ./aligned_GATK/Pr_1005_HiSeq2.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:227:H52T5BBXX:1\tPL:illumina\tPI:450\tLB:HiSeq2\tSM:Pr_1010" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq2/Pieris_HiSeq2_F_cleaned76_Pr_1010.fastq.gz > ./aligned_GATK/Pr_1010_HiSeq2.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1010_HiSeq2.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1010_HiSeq2.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1010_HiSeq2.bam
rm ./aligned_GATK/Pr_1010_HiSeq2.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:227:H52T5BBXX:1\tPL:illumina\tPI:450\tLB:HiSeq2\tSM:Pr_1014" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq2/Pieris_HiSeq2_F_cleaned76_Pr_1014.fastq.gz > ./aligned_GATK/Pr_1014_HiSeq2.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1014_HiSeq2.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1014_HiSeq2.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1014_HiSeq2.bam
rm ./aligned_GATK/Pr_1014_HiSeq2.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00363.11.HGCL5BBXX.6\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1022" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1022_Plate1_A.fastq.gz > ./aligned_GATK/Pr_1022_Plate1_A.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00317.73.HHJWTBBXX.4\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1022" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1022_Plate1_B.fastq.gz > ./aligned_GATK/Pr_1022_Plate1_B.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00114:227:H52T5BBXX:1\tPL:illumina\tPI:450\tLB:HiSeq2\tSM:Pr_1022" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/HiSeq2/Pieris_HiSeq2_F_cleaned76_Pr_1022.fastq.gz > ./aligned_GATK/Pr_1022_HiSeq2.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1022_Plate1_A.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1022_Plate1_A.bam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1022_Plate1_B.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1022_Plate1_B.bam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1022_HiSeq2.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1022_HiSeq2.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1022_Plate1_A.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1022_Plate1_B.bam
../samtools-1.5/samtools index  ./aligned_GATK/Pr_1022_HiSeq2.bam
rm ./aligned_GATK/Pr_1022_Plate1_A.sam
rm ./aligned_GATK/Pr_1022_Plate1_B.sam
rm ./aligned_GATK/Pr_1022_HiSeq2.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00363.11.HGCL5BBXX.6\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1023" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1023_Plate1_A.fastq.gz > ./aligned_GATK/Pr_1023_Plate1_A.sam
bwa mem -t 32 -T 30 -M -R "@RG\tID:K00317.73.HHJWTBBXX.4\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1023" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1023_Plate1_B.fastq.gz > ./aligned_GATK/Pr_1023_Plate1_B.sam
../samtools-1.5/samtools view -h -b ./aligned_GATK/Pr_1023_Plate1_A.sam | ../samtools-1.5/samtools sort > ./aligned_GATK/Pr_1023_Plate1_A.bam
