bwa mem -t 32 -T 30 -M -R "@RG\tID:K00317.73.HHJWTBBXX.4\tPL:illumina\tPI:450\tLB:Plate1\tSM:Pr_1024" ../Genomes/Pieris_rapae_assembly_v2/Pieris_assembly_v2.fa ../CleanedReads/SplittingReads/Pr_1024_Plate1_B.fastq.gz > ./aligned_GATK/Pr_1024_Plate1_B.sam
